<section>
  <form action="/sign-up" method="POST" novalidate>
    <div>
      <div>
        <label for="first_name">First name</label>
        <input id="first_name" name="first_name" type="text" value="<%= inputs.first_name %>" placeholder="Enter your first name">
      </div>
      <%- include('partials/_error', { field: 'first_name' }) %>
      <div>
        <label for="last_name">Last name</label>
        <input id="last_name" name="last_name" type="text" value="<%= inputs.last_name %>" placeholder=" Enter your last name">
      </div>
      <%- include('partials/_error', { field: 'last_name' }) %>
    </div>
    <div>
      <label for="email">Email</label>
      <input id="email" name="email" type="email" value="<%= inputs.email %>" placeholder="Enter your email">
    </div>
    <%- include('partials/_error', { field: 'email' }) %>
    <div>
      <label for="password">Password</label>
      <input id="password" name="password" type="password" value="<%= inputs.password %>" placeholder="Enter your password">
    </div>
    <%- include('partials/_error', { field: 'password' }) %>
    <div>
      <label for="confirm_password">Confirm password</label>
      <input id="confirm_password" name="confirm_password" type="password" value="<%= inputs.confirm_password %>" placeholder="Confirm password">
    </div>
    <%- include('partials/_error', { field: 'confirm_password' }) %>
    <button type="submit">Sign up</button>
  </form>
  <a href="/login">Already have an account? Login</a>
  <div>
    <span>or</span>
  </div>
  <div>
    <div>
      <a href="/auth/google">Continue with Google</a>
    </div>
  </div>

</section>